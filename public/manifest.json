{
  "manifest_version": 3,
  "name": "Garuchu Extension",
  "description": "Post schedule on Garoon to Discord",
  "author": "uniurchin",
  "version": "1.0.0",
  "action": {
    "default_title": "Garuchu Extension",
    "default_popup": "index.html",
    "default_icon": "icons/icon128.png"
  },
  "icons": {
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs", 
    "scripting", 
    "activeTab"
  ]
}